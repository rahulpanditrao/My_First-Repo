    <validationRules>
        <fullName>DOB_validation</fullName>
        <active>true</active>
        <description>age must be &gt; 18</description>
        <errorConditionFormula>Age__c &lt;20  || Age__c &gt; 31</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Please provide a valid Date of birth. Employee cannot be less than 18 years of Age.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Last_working_day</fullName>
        <active>true</active>
        <description>Last working date cannot be less than joining or resignation date.</description>
        <errorConditionFormula>Resignation_date__c  &lt;  Date_of_Joining__c</errorConditionFormula>
        <errorDisplayField>Resignation_date__c</errorDisplayField>
        <errorMessage>Last working date should be greater than joining date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation_date</fullName>
        <active>true</active>
        <description>if notice period isn&apos;t checked, it should give an error.</description>
        <errorConditionFormula>IF(AND(  Is_on_Notice_Period__c  == TRUE,  LEN( TEXT( Resignation_date__c)) == 0), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Resignation_date__c</errorDisplayField>
        <errorMessage>Please enter resignation date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_Number</fullName>
        <active>true</active>
        <description>limit size to 10</description>
        <errorConditionFormula>LEN( TEXT( SSN__c ) ) &gt; 10</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>Please enter a valid SSN.</errorMessage>
    </validationRules>
